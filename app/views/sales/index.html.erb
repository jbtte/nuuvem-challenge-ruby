<div class="container">
  <h1>Company Sales</h1>
  <hr>

  <%= form_tag '/sales', multipart: true do %>
    <%= file_field_tag :file %>
    <div class="mt-3"><%= submit_tag 'Upload' %></div>
  <% end %>
  
<hr>
  <h5>Total Gross income: $ <%=@gross_income%></h5>
  <h5> Last upload file gross income: $ <%=@last_gross_income%></h5>
</div>